{
  "name": "jomini",
  "version": "0.2.11",
  "description": "Parses Paradox files into javascript objects",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/jison -m commonjs -o ./lib/jomini.js ./lib/jomini.jison",
    "pretest": "npm run build",
    "test": "mocha test",
    "prepublishOnly": "npm test",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nickbabcock/jomini.git"
  },
  "author": "Nick Babcock <nbabcock19@hotmail.com>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.2.0",
    "concat-stream": "^2.0.0",
    "eslint": "^6.8.0",
    "jison": "^0.4.18",
    "mocha": "^6.1.4"
  },
  "homepage": "https://github.com/nickbabcock/jomini",
  "keywords": [
    "paradox",
    "clausewitz",
    "jomini",
    "parser",
    "javascript"
  ],
  "types": "index.d.ts",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 3
    },
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ]
  },
  "eslintIgnore": [
    "/lib/jomini.js"
  ]
}
